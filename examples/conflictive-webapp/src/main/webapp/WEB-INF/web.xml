<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>ConflictiveServlet</servlet-name>
        <servlet-class>es.guillermogonzalezdeaguero.container.conflictiveplugin.ConflictiveServlet</servlet-class>
        <init-param>
            <param-name>test_param</param-name>
            <param-value>This is the value</param-value>
        </init-param>
    </servlet>
    <servlet-mapping>
        <servlet-name>ConflictiveServlet</servlet-name>
        <url-pattern>/conflictive.html</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>MonitoringServlet</servlet-name>
        <servlet-class>es.guillermogonzalezdeaguero.container.examplelibrary.MonitoringServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>MonitoringServlet</servlet-name>
        <url-pattern>/monitoring.html</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>MonitoringServlet2</servlet-name>
        <servlet-class>es.guillermogonzalezdeaguero.container.examplelibrary.MonitoringServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>MonitoringServlet2</servlet-name>
        <url-pattern>/monitoring2.html</url-pattern>
    </servlet-mapping>
    
    <filter>
        <filter-name>Test</filter-name>
        <filter-class>es.guillermogonzalezdeaguero.container.conflictiveplugin.TestFilter</filter-class>
    </filter>
    
    <filter-mapping>
        <filter-name>Test</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>
</web-app>
